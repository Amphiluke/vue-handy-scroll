{
  "name": "vue-handy-scroll",
  "version": "1.0.1",
  "description": "Handy floating scrollbar component for Vue.js",
  "main": "dist/handy-scroll.umd.js",
  "module": "dist/handy-scroll.esm.js",
  "unpkg": "dist/handy-scroll.umd.js",
  "browser": {
    "./sfc": "src/handy-scroll.vue"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amphiluke/vue-handy-scroll.git"
  },
  "keywords": [
    "vue",
    "scrollbar",
    "component"
  ],
  "author": "Amphiluke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amphiluke/vue-handy-scroll/issues"
  },
  "homepage": "https://amphiluke.github.io/vue-handy-scroll/dist/",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "eslint": "^7.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "rollup": "^2.22.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": ">=2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}

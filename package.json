  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@vue/compiler-sfc": "^3.0.11",
    "eslint": "^7.24.0",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "postcss": "^8.2.9",
    "rollup": "^2.45.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
